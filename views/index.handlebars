<div class="container space">

    <form method="post"
          ref='uploadForm'
          id='uploadForm'
          method='post'
          encType="multipart/form-data"
          action="/tracks" class="form-horizontal" role="form">

        <div class="form-group">
            <label class="col-sm-3 control-label">Upload your track</label>
            <div class="col-sm-7">

                <input type="file" class="form-control input-lg" id="Username" placeholder="id" name="sampleFile">
            </div>
        </div>


        <div class="form-group">
            <label for="Password" class="col-sm-3 control-label">name</label>
            <div class="col-sm-7">
                <input type="text" class="form-control input-lg filestyle" id="Password" placeholder="name" name="name">
            </div>
        </div>


        <div class="form-group">
            <div class="col-sm-offset-3 col-sm-9">
                <button type="submit" onclick="add()" class="btn-lg singUp" value='Upload!'>Add</button>
            </div>
        </div>
    </form>
